extends ../layout

block content
  body
    h1 程氏星辉 用户列表页
    .container(style="margin-bottom:60%;")
      .row
        table.table.table-hover.table-bordered
          thead
            tr
              th 名字
              th 时间
          tbody
            each item in users
              tr(class="item-id-#{item._id}")
                td #{item.name}
                td #{moment(item.meta.updateAt).format('MM/DD/YYYY')}